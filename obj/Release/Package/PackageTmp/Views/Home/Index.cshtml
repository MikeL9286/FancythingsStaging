
<div class="SlideContainer">
    <div id="slide1" class="live">
        <img src="../../Images/arm_party.JPG" alt="Arm Party" />
    </div>
    <div id="slide2">
        <img src="../../Images/Baies_candle.jpeg" alt="Baies Candle" />
    </div>
    <div id="slide3">
        <img src="../../Images/book_shelves.JPG" alt="Book Shelves" />
    </div>
    <div id="slide4">
        <img src="../../Images/leopard_armparty.JPG" alt="Leopard Arm Party" />
    </div>
    <div id="slide5">
        <img src="../../Images/tulips.JPG" alt="Tulips" />
    </div>
    <div id="slide6">
        <img src="../../Images/zara_pumps.JPG" alt="Zara Pumps" />
    </div>
    <section class="SlideToolbar">
        <ul class="SlideBlobs">
            <li data-slideFor="slide1" onclick="ChangeSlide($(this).attr('data-slideFor'))" style="background-color:#f9bac3"></li>
            <li data-slideFor="slide2" onclick="ChangeSlide($(this).attr('data-slideFor'))"></li>
            <li data-slideFor="slide3" onclick="ChangeSlide($(this).attr('data-slideFor'))"></li>
            <li data-slideFor="slide4" onclick="ChangeSlide($(this).attr('data-slideFor'))"></li>
            <li data-slideFor="slide5" onclick="ChangeSlide($(this).attr('data-slideFor'))"></li>
            <li data-slideFor="slide6" onclick="ChangeSlide($(this).attr('data-slideFor'))"></li>
        </ul>
    </section>
    <div class="SlideToolbarShadow"></div>
</div>

@section blogPortal {
